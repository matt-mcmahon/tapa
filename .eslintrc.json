{
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
    // "parser": "babel-eslint"
  },
  "extends": "standard",
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "func-call-spacing": [
      "off",
      "always",
      {
        "allowNewlines": true
      }
    ]
  }
}
